#include<stdio.h>
int main(){
    int n,m,e=0,s=0;
    printf("enter row");
    scanf("%d",&n);
    
    printf("enter col");
    scanf("%d",&m);
    int a[n][m],i,j;
    for(i=0;i<n;i++){
        for(j=0;j<m;j++){
            scanf("%d",&a[i][j]);
        }
    }
    for(i=0;i<n;i++){
        for(j=0;j<m;j++){
            if(i==j){
                e=e+a[i][j];
            }
            else{
                s=s+a[i][j];
            }
         printf("%d",a[i][j]);
        }
        printf("\n");
    }
    printf("%d\n%d",e,s);
}

//sum of two matrices
#include<stdio.h>
int main(){
    int n,m;
    printf("enter row");
    scanf("%d",&n);
    
    printf("enter col");
    scanf("%d",&m);
    int a[n][m],b[n][m],c[n][m],i,j;
    for(i=0;i<n;i++){
        for(j=0;j<m;j++){
            scanf("%d",&a[i][j]);
        }
    }

    for(i=0;i<n;i++){
        for(j=0;j<m;j++){
            scanf("%d",&b[i][j]);
        }
    }
    
    for(i=0;i<n;i++){
        for(j=0;j<m;j++){
            c[i][j]=a[i][j]+b[i][j];        
        }
    }
    for(i=0;i<n;i++){
        for(j=0;j<m;j++){
            printf("%d",c[i][j]);
        }
        printf("\n");
    }
    
}

//max of 2d matrices
#include<stdio.h>
int main(){
    int n,m;
    printf("enter row");
    scanf("%d",&n);
    
    printf("enter col");
    scanf("%d",&m);
    int a[n][m],b[n][m],c[n][m],i,j;
    for(i=0;i<n;i++){
        for(j=0;j<m;j++){
            scanf("%d",&a[i][j]);
        }
    }
    int max=a[0][j];
    for(i=0;i<n;i++){
        for(j=0;j<m;j++){
            if(max<a[i][j]){
                max=a[i][j];
            }
        }
    }
    
    printf("%d",max);
}

//min of 2d matrices
#include<stdio.h>
int main(){
    int n,m;
    printf("enter row");
    scanf("%d",&n);
    
    printf("enter col");
    scanf("%d",&m);
    int a[n][m],b[n][m],c[n][m],i,j;
    for(i=0;i<n;i++){
        for(j=0;j<m;j++){
            scanf("%d",&a[i][j]);
        }
    }
    int min=a[0][j];
    for(i=0;i<n;i++){
        for(j=0;j<m;j++){
            if(min>a[i][j]){
                min=a[i][j];
            }
        }
    }
    
    printf("%d",min);
}


