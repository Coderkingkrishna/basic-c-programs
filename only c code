//upper triangular matrix
#include<stdio.h>
int main(){
    int n,m;
    printf("enter row");
    scanf("%d",&n);
    
    printf("enter col");
    scanf("%d",&m);
    int a[n][m],i,j;
    for(i=0;i<n;i++){
        for(j=0;j<m;j++){
            scanf("%d",&a[i][j]);
        }
    }
    for(i=0;i<n;i++){
        for(j=0;j<m;j++){
            if(i>j){
                a[i][j] = 0;
            }
        }
    }
    for(i=0;i<n;i++){
        for(j=0;j<m;j++){
         printf("%d",a[i][j]);
        }
        printf("\n");
    }
}

