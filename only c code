    for(i=0;i<n;i++){
        for(j=0;j<m;j++){
            scanf("%d",&a[i][j]);
        }
    }
    for(i=0;i<n;i++){
        for(j=0;j<m;j++){
            if(i<j){
                a[i][j] = 0;
            }
        }
    }
    for(i=0;i<n;i++){
        for(j=0;j<m;j++){
         printf("%d",a[i][j]);
        }
        printf("\n");
    }
}

//sum of lower triangular matrix
#include<stdio.h>
int main(){
    int n,m,s=0;
    printf("enter row");
    scanf("%d",&n);
    
    printf("enter col");
    scanf("%d",&m);
    int a[n][m],i,j;
    for(i=0;i<n;i++){
        for(j=0;j<m;j++){
            scanf("%d",&a[i][j]);
        }
    }
    for(i=0;i<n;i++){
        for(j=0;j<m;j++){
            if(i<j){
                a[i][j] = 0;
            }
        }
    }
    for(i=0;i<n;i++){
        for(j=0;j<m;j++){
         s += a[i][j];
        }
    
    }
    printf("%d",s);
}

//sum of upper triangular matrix
#include<stdio.h>
int main(){
    int n,m,s=0;
    printf("enter row");
    scanf("%d",&n);
    
    printf("enter col");
    scanf("%d",&m);
    int a[n][m],i,j;
    for(i=0;i<n;i++){
        for(j=0;j<m;j++){
            scanf("%d",&a[i][j]);
        }
    }
    for(i=0;i<n;i++){
        for(j=0;j<m;j++){
            if(i>j){
                a[i][j] = 0;
            }
        }
    }
    for(i=0;i<n;i++){
        for(j=0;j<m;j++){
         s += a[i][j];
        }
    
    }
    printf("%d",s);
}

//to check matrix is symmetric matrix or not
#include<stdio.h>
int main(){
    int n,m,z=0;
    printf("enter row");
    scanf("%d",&n);
    
    printf("enter col");
    scanf("%d",&m);
    int a[n][m],i,j;
    for(i=0;i<n;i++){
        for(j=0;j<m;j++){
            scanf("%d",&a[i][j]);
        }
    }
    for(i=0;i<n;i++){
        for(j=0;j<m;j++){
            if(a[i][j] == a[j][i]){
                z++;
            }
        }
    }
    if(z == n*m){
    printf("symmetric matrix");
    }
    else{
        printf("not a symmetric matrix");
    }
}

//to check matrix is sparse matrix or not
#include<stdio.h>
int main(){
    int n,m,z=0,y=0;
    printf("enter row");
    scanf("%d",&n);
    
    printf("enter col");
    scanf("%d",&m);
    int a[n][m],i,j;
    for(i=0;i<n;i++){
        for(j=0;j<m;j++){
            scanf("%d",&a[i][j]);
        }
    }
    for(i=0;i<n;i++){
        for(j=0;j<m;j++){
            if(a[i][j] == 0){
                z++;
            }
            else{
                y++;
            }
        }
    }
    if(z>y){
    printf("sparse matrix");
    }
    else{
        printf("not a sparse matrix");
    }
}

//cube of number
#include <stdio.h>
int cube(int a){
    return(a*a*a);
}
int main()
{
    
    int a,k;
    scanf("%d",&a);
    k =  cube(a);
    printf("%d",k);
    return 0;
}

//circle of number
#include <stdio.h>
float diameter(int a){
    return(a/2);
}
float circumfrance(int a){
    return(2*3.14*a);
}
float area(int a){
    return(3.14*a*a);
}
float main()
{
    
    float a,d,c,k;
    scanf("%f",&a);
    k =  diameter(a);
    c = circumfrance(a);
    d = area(a);
    printf("%f %f %f",k,c,d);
    return 0;
}

//check even or odd number
#include <stdio.h>
int check(int a){
    if(a%2 == 0){
        printf("even");
    }
    else{
        printf("odd");
    }
}
int main()
{
    
    int a,k;
    scanf("%d",&a);
    check(a);
    return 0;
}

// to find max and min
#include <stdio.h>
int find(int a,int b){
    if(a>b){
        printf("the maximum is %d\n",a);
        printf("the minimum is %d\n",b);
    }
    else{
        printf("the maximum is %d\n",b);
        printf("the minimum is %d\n",a);
    }
}
void main()
{
    
    int a,b;
    scanf("%d%d",&a,&b);
    find(a,b);
    return ;
}

//power of number
#include <stdio.h>
#include<math.h>
int power(int a,int b){
    int k;
    k=pow(a,b);
    return k ;
}
int main()
{
    
    int a,k,b;
    scanf("%d%d",&a,&b);
    printf("%d",power(a,b));
    return 0;
}

//two print natural number till n
#include <stdio.h>
#include<math.h>
int natural(int a){
    int i;
    for(i=1;i<=a;i++){
        printf("%d\n",i);
    }
}
int main()
{
    
    int a;
    scanf("%d",&a);
    natural(a);
    return 0;
}


#include <stdio.h>
 #include<math.h>
 int sum(int,int);
 void armstrong(int);
 void prime(int);
 void table(int);
 int sum(int x,int y){
     return(x+y);
 }
     void armstrong(int arm)
    {
     int n,rem,result=0;
     n=log(arm)+1;
     for(arm;arm!=0;arm=arm/10){
         rem=arm%10;
         result+=pow(rem,n);
        } 
     if(arm==result){
         printf("no is armstrong");
        }
     else{
         printf("no is not armstrong");
         }
    }
    void prime(int a){
    int ls=1,z;
    for(ls;ls<=a;ls++){
     if((a%ls)==0){
         z++;
        }   
    }
    if(z>=2){
        printf("no is prime");
    }
    else{
        printf("the no is not prime" );
    }
}
void table(int a){
    for(int i=1;i<=10;i++){
        printf("%d\n",i*a);
    }
}

 int main(){
     int a,k,result;
     printf("1 for sum\n");
     printf("2 for armstrong\n");
     printf("3 for table\n");
     printf("4 for table\n");
     printf("enter value for switch\n");
     scanf("%d",&a);
 switch(a){
     case 1:
     int no1;
     int no2;
     printf("enter value for add\n");
     scanf("%d",&no1);
     scanf("%d",&no2);
     k=sum(no1,no2);
     printf("%d sum of two no is\n",k); 
     break;
    case 2:
    int arm;
    printf("enter no for checking of armstrong\n");
    scanf("%d",&arm);
    armstrong(arm);
    break;
    case 3:
    int prm;
    printf("enter no for checking that it si prime or not\n");
    scanf("%d",&prm);
    prime(prm);
    break;
    case 4:
    int no;
    printf("enter no to print its table\n");
    scanf("%d",&no);
    table(no);
    break;
    default:
    printf("enter valid no\n");
    }
 }
 
 
//even and odd number
#include <stdio.h>
#include<math.h>
int even(int a){
    int i;
    for(i=0;i<=a;i++){
        if(i%2==0){
            printf("%d\n",i);
        }
    }
}
int odd(int a){
    int i;
    for(i=0;i<=a;i++){
        if(i%2!=0){
            printf("%d\n",i);
        }
    }
}
int main()
{
    
    int a,k;
    scanf("%d",&a);
    printf("the even numbers are:");
    even(a);
    
    printf("the odd numbers are:");
    odd(a);
    return 0;
}

//sum of natural number
#include <stdio.h>
#include<math.h>
int sum(int a){
    int i,s=0;
    for(i=1;i<=a;i++){
        s += i ;
    }
    printf("%d",s);
}
int main()
{
    
    int a;
    scanf("%d",&a);
    printf("the sum of numbers are:");
    sum(a);
    return 0;
}

//sum of even and odd number
#include <stdio.h>
#include<math.h>
int even(int a){
    int i,e=0;
    for(i=0;i<=a;i++){
        if(i%2==0){
            e += i;
        }
    }
    printf("%d",e);
}
int odd(int a){
    int i,o=0;
    for(i=0;i<=a;i++){
        if(i%2!=0){
            o +=i;
        }
    }
    printf("%d",o);
}
int main()
{
    
    int a,k;
    scanf("%d",&a);
    printf("the sum of even numbers are:");
    even(a);
    printf("\n");
    printf("the sum of odd numbers are:");
    odd(a);
    return 0;
}

//reverse of number
#include <stdio.h>
#include<math.h>
int rev(int a){
    int i;
    for(i=a;i>=0;i--){
        printf("%d\n",i);

    }
}
int main()
{
    
    int a,k;
    scanf("%d",&a);
    printf("the rev of numbers are:");
    rev(a);
    return 0;
}

//pallindrome of number
#include <stdio.h>
#include<math.h>
int pal(int a){
    int rev=0,x=a; 
    while(a>0){
        rev=(rev*10)+(a%10);
        a /= 10;
    }
    if(x==rev){
        printf("pallindrome");
    }
    else{
        printf("not a pallindrome");
    }
}
int main()
{
    
    int a;
    scanf("%d",&a);
    pal(a);
    return 0;
}

//sum of digit of number
#include <stdio.h>
#include<math.h>
int dig(int a){
    int rev=0,x=a; 
    while(a>0){
        rev=(rev)+(a%10);
        a /= 10;
    }
    printf("%d",rev);
}
int main()
{
    
    int a;
    scanf("%d",&a);
    dig(a);
    return 0;
}

//factorial of number
#include <stdio.h>
#include<math.h>
int fac(int a){
    int i,f=1; 
    for(i=1;i<=a;i++){
        f=f*i;
    }
    printf("%d",f);
}
int main()
{
    
    int a;
    scanf("%d",&a);
    fac(a);
    return 0;
}

//fabonacci of number
#include <stdio.h>
#include<math.h>
int fab(int a){

	int n=0,i=0,j=1,k;
	printf("%d %d ",i,j);
	while(n<=(a-3))
	{
		k=i+j;
		i=j;
		j=k;
		printf("%d ",k);
		n++;
	}

}
int main()
{
    
    int a;
    scanf("%d",&a);
    fab(a);
    return 0;
}

//LCM of number
#include <stdio.h>
#include<math.h>
int lcm(int a,int b){
    int i;
    if(a>b){
        i=a;
    }
    else{
        i=b;
    }
    while(i<=(a*b)){
        if((i%a==0) && (i%b==0)){
            break;
            
        }
            i++;
        }
             printf("%d",i);
       
    
}
int main()
{
    
    int a,b;
    scanf("%d%d",&a,&b);
    lcm(a,b);
    return 0;
}

//hcf of number
#include <stdio.h>
#include<math.h>
int hcf(int a,int b){
    int i,hcf;
    if(a>b){
        i=a;
    }
    else{
        i=b;
    }
    while(i<=(a*b)){
        if((i%a==0) && (i%b==0)){
            break;
            
        }
            i++;
        }
        hcf = (a*b)/i;
       printf("%d",hcf);
    
}
int main()
{
    
    int a,b;
    scanf("%d%d",&a,&b);
    hcf(a,b);
    return 0;
}

//to diplay element  of array
#include <stdio.h>
#include<math.h>
int array(int a){
    int arr[a],i=0;
    for(i=0;i<=a;i++){
        scanf("%d",&arr[i]);
    }
    for(i=0;i<=a;i++){
        printf("%d ",arr[i]);
    }
}
int main()
{
    
    int a;
    scanf("%d",&a);
    array(a);
    return 0;
}

//to sum the element  of array
#include <stdio.h>
#include<math.h>
int sarray(int a){
    int arr[a],i,s=0;
    for(i=0;i<=a;i++){
        scanf("%d",&arr[i]);
        s += arr[i];
    }
        printf("%d ",s);
}
int main()
{
    
    int a;
    scanf("%d",&a);
    sarray(a);
    return 0;
}

//to find max and min element of array
#include <stdio.h>
#include<math.h>
int mxmn(int a){
    int arr[a],i;
    for(i=0;i<=a;i++){
        scanf("%d",&arr[i]);
    }
    int max = arr[0];
    for(i=0;i<=a;i++){
        if(max<arr[i]){
            max=arr[i];
        }
    }
    int min = arr[0];
    for(i=0;i<=a;i++){
        if(min>arr[i]){
            min=arr[i];
        }
    }
    printf("the max element of array %d\n",max);
    printf("the min element of array %d",min);
}

int main()
{
    
    int a;
    scanf("%d",&a);
    mxmn(a);
    return 0;
}

// print prime number in given range
#include<stdio.h>
int prime(int s,int e){
    int k,i,j;
    for(i=s;i<=e;i++){
        k=0;
        for(j=1;j<=i;j++){
            if(i%j==0){
              k+=1;
            }
        }
        if(k==2){
            printf("%d\n",i);
        }
    }
}
int main(){
    int s,e;
    printf("enter limits to check");
    scanf("%d",&s);
    scanf("%d",&e);
    prime(s,e);
    return 0;
}

// to check armstrong number
#include <stdio.h>
#include<math.h>
int arm(int a){
    
        int b,c,i,rev=0,x=a;
        b=log10(a)+1;
        for(i=0;a>0;i++){
            rev=rev+pow(a%10,b);
            a /= 10;
        }
        if(x==rev){
            printf("armstrong");
        }
        else{
            printf("not");
        }
    
}
int main()
{
    
    int a,k;
    scanf("%d",&a);
    arm(a);
    return 0;
}

//to check the number is prime number or not
#include <stdio.h>
int prime(int a){
    int i=1,c;
    for(i;i<=a;i++){
     if((a%i)==0){
         c++;
        }   
    }
    if(c==2){
        printf("no is prime");
    }
    else{
        printf(" no is not prime" );
    }
}
int main()
{
    int a;
    scanf("%d",&a);
    prime(a);
    return 0;
}

#include <stdio.h>

int main()
{
    int i,j,n,m,ch;
    scanf("%d",&n);
    scanf("%d",&m);
    int a[n][m];
    if(n!=m){
        printf("not possible");
    }
    else{
        for(i=0;i<n;i++){
            for(j=0;j<m;j++){
                scanf("%d",&a[i][j]);   
            }
        }
        
        for(i=0;i<n;i++){
            for(j=0;j<m;j++){
                printf("%d",a[i][j]);
            }
            printf("\n");
        }
        
        for(i=0;i<n;i++){
                ch = a[i][i];
                a[i][i] = a[i][n-i-1];
                a[i][n-i-1] = ch;
        }
        
        for(i=0;i<n;i++){
            for(j=0;j<m;j++){
                printf("%d",a[i][j]);
            }
            printf("\n");
        }
    }
    return 0;
}


