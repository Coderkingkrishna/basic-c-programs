#include<stdio.h>
int main()
#for reverse
{
	int a,b,s=0,x;
	scanf("%d",&a);
	x=a;
	while(a>0)
	{
		b=a%10;
		s=(s*10)+b;
		a=a/10;
		
	}
	printf("%d",s);
}

# for pallindrome

{
	int a,b,s=0,x;
	scanf("%d",&a);
	x=a;
	while(a>0)
	{
		b=a%10;
		s=(s*10)+b;
		a=a/10;
		
	}
	
	if(x==s)
	{
		printf("Pallindrome.");
	}
	else{
		printf("Not in Pallindrome.");
	}
}



# for factorial
{
    int i=1,n,m;
	scanf("%d",&n);
	while(n>=1)
	{
		i=i*n;
		n--;
		
		
}
printf("%d",i);
		
		

}
{
	int n,i=1;
	scanf("%d",&n);
	
	
	for(i=1;i<=n;i++)
	{
	if(n%i==0)
	{
		printf("%d \n",i);
		}	
	}

}




# for fabbonacci series
{
	int i=0,j=1,k,n;
	scanf("%d",&n);
	printf("%d %d ",i,j);
	while(i<=(n-2))
	{
		k=i+j;
		i=j;
		j=k;
		printf("%d ",k);
	}
}

